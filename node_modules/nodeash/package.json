{
  "name": "nodeash",
  "version": "2.3.9",
  "description": "nodeash is node js server package",
  "author": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "umd:main": "dist/umd/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "publish_app": "npm run build:upgrade && npm publish --access public .",
    "test": "npm run build:cjs && mocha ./dist/cjs/test/**/*.js --timeout -1",
    "build:upgrade": "npm --no-git-tag-version version patch",
    "build": "npm run build:cjs && npm run build:esm &&  npm run build:types",
    "build:cjs": "rm -rf ./dist/cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "rm -rf ./dist/esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "rm -rf ./dist/umd && webpack --config config/webpack.config.js",
    "build:types": "rm -rf ./dist/types && tsc -p config/tsconfig.types.json",
    "start": "node ./dist/cjs/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/akhilxyz/nodeash/tree/main"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "chai": "^4.3.10",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "fs": "^0.0.1-security",
    "helmet": "^7.1.0",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.0",
    "@types/sinon": "^10.0.15",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
